public class TestGenerics {
	public static void main(String[] args) {
		ValueHolder<Integer> holder=new ValueHolder<>(4);
	}
	
	public class ValueHolder<E>{
		E e;

		public ValueHolder(E e) {
			this.e = e;
		}
		
		public E getvalue() {
			return (E) ("I am value holder of"+e.toString());
		}
		public E replace () {
			
			return e;
		}
	}
	
}
