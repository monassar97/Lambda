
public class Account {
	private int balance;
	public  void draw(int value) {
		balance+=value;
	}
	public  void deposit(int value) {
		if(balance-value<0)
			System.out.println("Not Enough balance!");
		else {
			balance-=value;	
		}
		
	}
	public static void main(String[] args) {
		draw(1000);
	}
	
}
