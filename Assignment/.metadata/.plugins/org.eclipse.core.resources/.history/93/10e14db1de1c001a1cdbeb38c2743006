import java.util.PriorityQueue;
import java.util.Queue;

public class TestC {
	static Queue<Integer> prior = new PriorityQueue<Integer>(6);

	public static void getMedian() {
		if(prior.size()%2==0) {
			//even
			int first=0;
		for(int i=0;i<prior.size()/2;i++) {
			
			first=prior.poll();
		}
		System.out.println();
		//odd
		}else {
			
		}
		
	}

	public static void main(String[] args) {
		prior.add(1);
		prior.add(2);
		prior.add(3);
		prior.add(4);
		prior.add(5);
		prior.add(6);
		prior.add(7);
		prior.stream().forEach(System.out::println);
	}
}
